{
  "name": "@project/worker",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "nest start --watch --preserveWatchOutput",
    "start": "node dist/main",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "build": "nest build",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "test:unit": "vitest run --dir src",
    "test:e2e": "vitest run --dir test",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix"
  },
  "dependencies": {
    "@bull-board/api": "^6.16.2",
    "@bull-board/express": "^6.16.2",
    "@bull-board/ui": "^6.16.2",
    "@google-cloud/speech": "^7.2.1",
    "@google-cloud/storage": "^7.18.0",
    "@google-cloud/video-intelligence": "^6.2.1",
    "@google-cloud/video-transcoder": "^4.4.1",
    "@nestjs/bull": "^11.0.4",
    "@nestjs/bullmq": "^11.0.4",
    "@nestjs/common": "^11.1.12",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.12",
    "@nestjs/platform-express": "^11.1.12",
    "@nestjs/schedule": "^6.1.0",
    "@nestjs/terminus": "^11.0.0",
    "@project/shared": "workspace:*",
    "bull": "^4.16.5",
    "bullmq": "^5.66.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "express": "^5.2.1",
    "ioredis": "^5.9.2",
    "joi": "^18.0.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "tsx": "^4.21.0",
    "zod": "^4.3.5"
  },
  "packageManager": "yarn@4.12.0",
  "devDependencies": {
    "@darraghor/eslint-plugin-nestjs-typed": "^7.1.14",
    "@nestjs/cli": "^11.0.16",
    "@nestjs/testing": "^11.1.12",
    "@types/express": "^5.0.6",
    "@types/joi": "^17.2.3",
    "@types/node": "^25.0.9",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "@vitest/coverage-v8": "^4.0.17",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "fast-check": "^4.5.3",
    "nodemon": "^3.1.11",
    "prettier": "^3.8.0",
    "supertest": "^7.2.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17"
  }
}

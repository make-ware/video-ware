{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true
  },
  "files": [],
  "references": [
    {
      "path": "./shared"
    },
    {
      "path": "./webapp"
    }
  ],
  "exclude": ["node_modules", "**/node_modules", "**/dist", "**/.next", "pb"]
}



